var(type, name) ::= "<type> <name>;"

struct(name, members) ::= <<
struct <name>
{
    <members>
};
>>

cbuffer(name, members, reg, space) ::= <<
cbuffer <name> : register(b<reg>, space<space>)
{
    <members>
};
>>

include(path) ::= "#include <path>;"

sampler(name, bindpoint, space) ::= "SamplerState <name> : register(s<bindpoint>, space<space>);"